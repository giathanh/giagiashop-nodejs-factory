{
  "name": "giagiashop-nodejs-factory",
  "description": "factory for nodejs giagiashop",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "dependencies": {
    "http-status": "^1.4.2",
    "setprototypeof": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint && npm run prettier && npm audit",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./coverage ./.nyc_output",
    "doc": "jsdoc -c jsdoc.json",
    "test": "npm run check",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giathanh/giagiashop-nodejs-factory.git"
  },
  "author": "thanh.clg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/giathanh/giagiashop-nodejs-factory/issues"
  },
  "homepage": "https://github.com/giathanh/giagiashop-nodejs-factory#readme"
}
